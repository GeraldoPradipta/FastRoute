open "generic" {
	"lefFiles" : "example.lef",
	"defFiles" : "example.def"
};

run "rsyn.fastRoute" {"outfile" : "example.guide", "adjustment" : 0.X, "minRoutingLayer" : Y, "maxRoutingLayer" : Z, "unidirectionalRoute" : B};

# outfile: string. indicate the name of the generated guides file
# adjustment: float. indicate the percentage reduction of each edge
# minRoutingLayer: integer. indicate the min routing layer available for FastRoute
# maxRoutingLayer: integer. indicate the max routing layer available for FastRoute
# unidirectionalRoute: boolean. indicate if unidirectional routing is activated